{
  "name": "TCR",
  "manifest_version":2,
  "version":"1.0",
  "description": "Test code reviewer",
  "permissions": ["tabs","activeTab", "*://*.google.com/*","storage","*://*/*"],
  "author":"Aditya Jain",
  "options_ui": {
  "page": "lib/options.html",
  "chrome_style": true
},
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_end",
      "js": ["jumble.js","toggle.js","clienttest.js","lib/jquery-2.1.3.min.js"]
    }
  ]
}
